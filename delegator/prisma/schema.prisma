generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id          Int     @id @default(autoincrement())
  from        String
  to          String
  input       String
  gas         String
  value       String
  chainId     String
  gasPrice    String
  nonce       String
  v           String
  r           String
  s           String
  rawTx       String
  signedRawTx String?
}
